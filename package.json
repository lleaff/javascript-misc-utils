{
  "name": "javascript-misc-utils",
  "version": "1.0.0",
  "description": "Miscellaneous cross-platform (Browser/Node) JavaScript utilities",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lleaff/javascript-misc-utils.git"
  },
  "keywords": [
    "utilities"
  ],
  "author": "lleaff",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lleaff/javascript-misc-utils/issues"
  },
  "homepage": "https://github.com/lleaff/javascript-misc-utils#readme",
  "main": "./lib/index.js",
  "scripts": {
    "start": "gulp",
    "watch": "gulp watch",
    "clean": "rm -r ./lib/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-destructuring": "^6.18.0",
    "babel-plugin-transform-flow-comments": "^6.17.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-watch": "^2.0.3",
    "flow-bin": "^0.34.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "babel-plugin-transform-flow-comments",
      "add-module-exports",
      "transform-es2015-destructuring",
      "transform-object-rest-spread",
      "transform-function-bind"
    ]
  }
}
