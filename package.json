{
  "name": "javascript-misc-utils",
  "version": "1.0.0",
  "description": "Miscellaneous cross-platform (Browser/Node) JavaScript utilities",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lleaff/javascript-misc-utils.git"
  },
  "keywords": [
    "utilities"
  ],
  "author": "lleaff",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lleaff/javascript-misc-utils/issues"
  },
  "homepage": "https://github.com/lleaff/javascript-misc-utils#readme",
  "main": "./lib/index.js",
  "scripts": {
    "start": "babel --source-maps --out-dir=lib src",
    "watch": "babel-watch --source-maps --out-dir=lib src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-destructuring": "^6.18.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-watch": "^2.0.3"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "transform-es2015-destructuring",
      "transform-object-rest-spread",
      "transform-function-bind"
    ]
  }
}
